

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model Equipo {
  id Int @id @default(autoincrement())
  nombre String
  patente String
  chofer String
  marca String
  modelo String
  kilometraje Float 
  imagen String
}



model Faena {
  id Int @id @default(autoincrement())
  nombre String
}

model Orden {
  id Int @id @default(autoincrement())
  folio String
  nombre String
  cantidad String
  descripcion String
  fecha DateTime
  patente String
  pedido Json
  obra String
  valor String
  pedido01 Json
  emisor String
  Kilometraje String
  propietario String
  estado Boolean  @default(false)
  finalizado Boolean  @default(false)
  anular Boolean @default(false)
  estado01 Boolean  @default(false)
}



model Ocpedidos {
  id Int @id @default(autoincrement())
  folio String
  nombre String
  nombre01 String?
  rut01 String?
  emisor String
  obra String
  cantidad String 
  descripcion String 
  valor String
  cantidad01 String?
  descripcion01 String?
  valor01 String?
  cantidad02 String ?
  descripcion02 String?
  valor02 String ?
  cantidad03 String ?
  descripcion03 String?
  valor03 String ?
  cantidad04 String ?
  descripcion04 String?
  valor04 String?
  orden Json
  fecha DateTime
  fechasolicitud String
  estado Boolean @default(false)
  anular Boolean @default(false)
  finalizado Boolean @default(false)
  estado01 Boolean  @default(false)
}


model User {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  password String
}



model Proveedores {
  id Int @id @default(autoincrement())
  nombre String
  direccion String
  rut String
  fono String
  comuna String
}


model Descripcion {
  id Int @id @default(autoincrement())
  nombre String
}



model Obra {
  id Int @id @default(autoincrement())
  nombre String
  cantidad String
  estado  Boolean  @default(false)
}



model OrdenObra {
  id Int @id @default(autoincrement())
  nombre String
  cantidad String
  obra String
  descripcion String
  fecha DateTime
  patente String
  pedido Json
  pedido03 Json
  emisor String
  Kilometraje String
  propietario String
  estado Boolean  @default(false)
  finalizado Boolean  @default(false)
  anular Boolean @default(false)
  estado01 Boolean  @default(false)
}


















